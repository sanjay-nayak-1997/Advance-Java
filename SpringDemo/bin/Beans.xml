<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
<bean id="comp" class="com.deloitte.Computer">
<property name="brandname" value="ASUS"></property>
<constructor-arg ref="cpu1"></constructor-arg>
</bean>

<bean id="cpu1" class="com.deloitte.CPU">
<property name="cpubrand" value="INTEL"></property>
</bean>
</beans>
   